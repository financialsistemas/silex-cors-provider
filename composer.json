{
    "name": "jdesrosiers/silex-cors-provider",
    "description": "A silex service provider that adds CORS services to silex",
    "keywords": ["silex", "cors", "cross", "origin", "resource", "sharing", "service", "provider"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Desrosiers",
            "email": "jdesrosi@gmail.com"
        }
    ],
    "require": {
        "silex/silex": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "symfony/browser-kit": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/routing": "^5.0"
    },
    "repositories": [
        {
            "url": "https://github.com/financialsistemas/Silex.git",
            "type": "vcs"
        }
    ],
    "autoload": {
        "psr-4": {
            "JDesrosiers\\Silex\\Provider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JDesrosiers\\Silex\\Provider\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors"
    }
}